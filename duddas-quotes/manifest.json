{
  "manifest_version": 3,
  "name": "Duddas Quotes 1.0",
  "version": "1.0",
  "description": "AI-powered quote tool for health insurance sales. Get instant quotes and AI suggestions.",
  "permissions": ["storage"],
  "host_permissions": ["https://www.salesgodcrm.net/*", "https://ai-lead-system-production-df0a.up.railway.app/*"],
  "content_scripts": [
    {
      "matches": ["https://www.salesgodcrm.net/messenger*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
