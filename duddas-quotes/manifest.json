{
  "manifest_version": 3,
  "name": "Duddas Quotes 2.0",
  "version": "2.0.0",
  "description": "Quick quote calculator for health insurance sales.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
